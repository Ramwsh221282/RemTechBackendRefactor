# Code quality analyzers
dotnet_analyzer_diagnostic.category-security.severity = error # Все предупреждения по безопасности — ошибка
dotnet_code_quality.ca1802.api_surface = private, internal # Применять CA1802 только к private/internal


# name all constant fields using PascalCase
dotnet_naming_rule.constant_fields_should_be_pascal_case.severity = suggestion # Предупреждение, если константа не в PascalCase
dotnet_naming_rule.constant_fields_should_be_pascal_case.symbols = constant_fields # Применять к символам constant_fields
dotnet_naming_rule.constant_fields_should_be_pascal_case.style = pascal_case_style # Стиль PascalCase
dotnet_naming_symbols.constant_fields.applicable_kinds = field # Применять к полям
dotnet_naming_symbols.constant_fields.required_modifiers = const # Только для const
dotnet_naming_style.pascal_case_style.capitalization = pascal_case # PascalCase стиль


# static fields should have s_ prefix
dotnet_naming_rule.static_fields_should_have_prefix.severity = suggestion # Предупреждение, если статическое поле без префикса s_
dotnet_naming_rule.static_fields_should_have_prefix.symbols = static_fields # Применять к символам static_fields
dotnet_naming_rule.static_fields_should_have_prefix.style = static_prefix_style # Стиль static_prefix_style
dotnet_naming_symbols.static_fields.applicable_kinds = field # Применять к полям
dotnet_naming_symbols.static_fields.required_modifiers = static # Только для static
dotnet_naming_symbols.static_fields.applicable_accessibilities = private, internal, private_protected # Для приватных/внутренних
dotnet_naming_style.static_prefix_style.required_prefix = s_ # Префикс s_
dotnet_naming_style.static_prefix_style.capitalization = camel_case # Стиль camelCase


# internal and private fields should be _camelCase
dotnet_naming_rule.camel_case_for_private_internal_fields.severity = suggestion # Предупреждение, если приватное/внутреннее поле не _camelCase
dotnet_naming_rule.camel_case_for_private_internal_fields.symbols = private_internal_fields # Применять к символам private_internal_fields
dotnet_naming_rule.camel_case_for_private_internal_fields.style = camel_case_underscore_style # Стиль camel_case_underscore_style
dotnet_naming_symbols.private_internal_fields.applicable_kinds = field # Применять к полям
dotnet_naming_symbols.private_internal_fields.applicable_accessibilities = private, internal # Для приватных/внутренних
dotnet_naming_style.camel_case_underscore_style.required_prefix = _ # Префикс _
dotnet_naming_style.camel_case_underscore_style.capitalization = camel_case # Стиль camelCase


# Code quality
dotnet_style_readonly_field = true:suggestion # Предпочитать readonly для полей
dotnet_code_quality_unused_parameters = non_public:suggestion # Предупреждать о неиспользуемых не-публичных параметрах

# Expression-level preferences
dotnet_style_object_initializer = true:suggestion # Предпочитать инициализацию объектов через { ... }
dotnet_style_collection_initializer = true:suggestion # Предпочитать инициализацию коллекций через { ... }
dotnet_style_explicit_tuple_names = true:suggestion # Предпочитать явные имена для элементов кортежей
dotnet_style_coalesce_expression = true:suggestion # Предпочитать использование ?? для проверки на null
dotnet_style_null_propagation = true:suggestion # Предпочитать ?. для обращения к членам объектов, которые могут быть null
dotnet_style_prefer_is_null_check_over_reference_equality_method = true:suggestion # Предпочитать is null вместо object.ReferenceEquals
dotnet_style_prefer_inferred_tuple_names = true:suggestion # Предпочитать неявные имена для кортежей (var (a, b) = ...)
dotnet_style_prefer_inferred_anonymous_type_member_names = true:suggestion # Предпочитать неявные имена для анонимных типов
dotnet_style_prefer_auto_properties = true:suggestion # Предпочитать авто-свойства вместо полей с ручной реализацией
dotnet_style_prefer_conditional_expression_over_assignment = true # Предпочитать тернарный оператор ?: вместо if-else при присваивании
dotnet_style_prefer_conditional_expression_over_return = true # Предпочитать тернарный оператор ?: вместо if-else при возврате значения

# C# files
[*.cs]
# New line preferences
csharp_new_line_before_open_brace = all
csharp_new_line_before_else = true
csharp_new_line_before_catch = true
csharp_new_line_before_finally = true
csharp_new_line_before_members_in_object_initializers = true
csharp_new_line_before_members_in_anonymous_types = true
csharp_new_line_between_query_expression_clauses = true

# Indentation preferences
csharp_indent_block_contents = true
csharp_indent_braces = false
csharp_indent_case_contents = true
csharp_indent_case_contents_when_block = true
csharp_indent_switch_labels = true
csharp_indent_labels = one_less_than_current

# Modifier preferences
csharp_preferred_modifier_order = public, private, protected, internal, static, extern, new, virtual, abstract, sealed, override, readonly, unsafe, volatile, async:suggestion

# Code style defaults
csharp_using_directive_placement = outside_namespace:error # using-директивы вне пространства имён
csharp_prefer_braces = true # Всегда использовать фигурные скобки для if/for/while и т.д.
csharp_preserve_single_line_blocks = true # Сохранять однострочные блоки кода
csharp_preserve_single_line_statements = false # Не сохранять однострочные операторы
csharp_prefer_static_local_function = true:suggestion # Предпочитать static для локальных функций
csharp_prefer_simple_using_statement = false:none # Не использовать сокращённый using (C# 8)
csharp_style_prefer_switch_expression = true:suggestion # Предпочитать switch-выражения вместо switch-блоков

# Expression-bodied members
csharp_style_expression_bodied_methods = true # Предпочитать методы с выражением-результатом (=>)
csharp_style_expression_bodied_constructors = true # Предпочитать конструкторы с выражением-результатом
csharp_style_expression_bodied_operators = true # Предпочитать операторы с выражением-результатом
csharp_style_expression_bodied_properties = true # Предпочитать свойства с выражением-результатом
csharp_style_expression_bodied_indexers = true # Предпочитать индексаторы с выражением-результатом
csharp_style_expression_bodied_accessors = true # Предпочитать аксессоры с выражением-результатом
csharp_style_expression_bodied_lambdas = true # Предпочитать лямбда-выражения с выражением-результатом
csharp_style_expression_bodied_local_functions = true # Предпочитать локальные функции с выражением-результатом

# Pattern matching
csharp_style_pattern_matching_over_is_with_cast_check = true:suggestion # Предпочитать pattern matching вместо is + cast
csharp_style_pattern_matching_over_as_with_null_check = true:suggestion # Предпочитать pattern matching вместо as + null check
csharp_style_inlined_variable_declaration = true:suggestion # Предпочитать объявление переменной прямо в pattern matching

# Expression-level preferences
csharp_prefer_simple_default_expression = true:suggestion # Предпочитать default(T) вместо new T() для значимых типов

# Null checking preferences
csharp_style_throw_expression = true:suggestion # Использовать throw-выражения (например, ?? throw)
csharp_style_conditional_delegate_call = true:suggestion # Использовать ?. для вызова делегатов

# Other features
csharp_style_prefer_index_operator = false:none # Не использовать индексатор ^ и ..
csharp_style_prefer_range_operator = false:none # Не использовать range-оператор ..
csharp_style_pattern_local_over_anonymous_function = false:none # Не использовать локальные функции вместо анонимных

# Space preferences
csharp_space_after_cast = false # Нет пробела после приведения типа: (int)x
csharp_space_after_colon_in_inheritance_clause = true # Пробел после двоеточия в наследовании: class A : B
csharp_space_after_comma = true # Пробел после запятой в списках
csharp_space_after_dot = false # Нет пробела после точки
csharp_space_after_keywords_in_control_flow_statements = true # Пробел после ключевых слов if, for, while и т.д.
csharp_space_after_semicolon_in_for_statement = true # Пробел после точки с запятой в for
csharp_space_around_binary_operators = before_and_after # Пробелы до и после бинарных операторов: a + b
csharp_space_before_colon_in_inheritance_clause = true # Пробел перед двоеточием в наследовании: class A : B
csharp_space_before_comma = false # Нет пробела перед запятой
csharp_space_before_dot = false # Нет пробела перед точкой
csharp_space_before_open_square_brackets = false # Нет пробела перед открывающей квадратной скобкой
csharp_space_before_semicolon_in_for_statement = false # Нет пробела перед точкой с запятой в for
csharp_space_between_empty_square_brackets = false # Нет пробела между пустыми квадратными скобками: arr[]
csharp_space_between_method_call_empty_parameter_list_parentheses = false # Нет пробела между пустыми скобками вызова метода: Foo()
csharp_space_between_method_call_name_and_opening_parenthesis = false # Нет пробела между именем метода и скобкой: Foo()
csharp_space_between_method_call_parameter_list_parentheses = false # Нет пробела внутри скобок параметров вызова метода
csharp_space_between_method_declaration_empty_parameter_list_parentheses = false # Нет пробела между пустыми скобками объявления метода
csharp_space_between_method_declaration_name_and_open_parenthesis = false # Нет пробела между именем метода и скобкой при объявлении
csharp_space_between_method_declaration_parameter_list_parentheses = false # Нет пробела внутри скобок параметров объявления метода
csharp_space_between_parentheses = false # Нет пробелов внутри любых скобок
csharp_space_between_square_brackets = false # Нет пробелов внутри квадратных скобок

# Types: use keywords instead of BCL types, and permit var only when the type is clear
csharp_style_var_for_built_in_types = false:suggestion # Не использовать var для встроенных типов (int, string и т.д.)
csharp_style_var_when_type_is_apparent = false:none # Не использовать var, даже если тип очевиден из правой части
csharp_style_var_elsewhere = false:suggestion # Не использовать var в остальных случаях (требовать явное указание типа)

# Xml project files
[*.{csproj,vbproj,vcxproj,vcxproj.filters,proj,nativeproj,locproj}]
indent_size = 2

# Xml build files
[*.builds]
indent_size = 2

# Xml files
[*.{xml,stylecop,resx,ruleset}]
indent_size = 2

# Xml config files
[*.{props,targets,config,nuspec}]
indent_size = 2

# Shell scripts
[*.sh]
end_of_line = lf

[*.{cmd, bat}]
end_of_line = crlf

[{*.har,*.jsb2,*.jsb3,*.json,*.jsonc,*.postman_collection,*.postman_collection.json,*.postman_environment,*.postman_environment.json,.babelrc,.eslintrc,.prettierrc,.stylelintrc,.ws-context,bowerrc,jest.config}]
indent_style = space
indent_size = 2

[{*.yaml,*.yml}]
indent_style = space
indent_size = 2

[*.{appxmanifest,asax,ascx,aspx,axaml,build,c,c++,c++m,cc,ccm,cginc,compute,cp,cpp,cppm,cs,cshtml,cu,cuh,cxx,cxxm,dtd,fs,fsi,fsscript,fsx,fx,fxh,h,hh,hlsl,hlsli,hlslinc,hpp,hxx,inc,inl,ino,ipp,ixx,master,ml,mli,mpp,mq4,mq5,mqh,mxx,nuspec,paml,razor,resw,resx,shader,skin,tpp,usf,ush,uxml,vb,xaml,xamlx,xoml,xsd}]
indent_style = space
indent_size = 4
tab_width = 4

