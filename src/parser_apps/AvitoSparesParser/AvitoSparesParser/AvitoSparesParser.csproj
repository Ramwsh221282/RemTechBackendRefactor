        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>
        
    <ItemGroup>
        <Using Include="ParsingSDK" />
        <Using Include="AvitoFirewallBypass" />
        <Using Include="ParserSubscriber" />
